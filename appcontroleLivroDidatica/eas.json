{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_USE_CASE_SENSITIVE_PATHS": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_USE_CASE_SENSITIVE_PATHS": "true"
      },
      "extends": "production",
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "env": {
        "EXPO_USE_CASE_SENSITIVE_PATHS": "true"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
} 